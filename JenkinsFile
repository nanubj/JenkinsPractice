pipeline {
    agent any

    stages {
        stage('Test') {
            agent{label 'L1'}
            steps {
                echo 'Testing'
            }
        }
        
        stage('Parallel'){
            parallel{
                stage('V1'){
                    steps{
                    echo 'V1 Parallel'
                    }
                }
                
                stage('V2'){
                    steps{
                    echo 'V2 Parallel'
                    }
                }
                
            }
        }
        
    }
}
